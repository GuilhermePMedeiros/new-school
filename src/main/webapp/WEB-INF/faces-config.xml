<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	    <navigation-rule>
			<from-view-id>/*</from-view-id>
			<navigation-case>
			    <from-outcome>login</from-outcome>
			    <to-view-id>./pages/login/login.xhtml</to-view-id>
			    <redirect/>
		 	</navigation-case>
			<navigation-case>
			    <from-outcome>home</from-outcome>
			    <to-view-id>./restricted/dashboard.xhtml</to-view-id>
			    <redirect/>
		 	</navigation-case>
		 	<navigation-case>
		    	<from-outcome>studentForm</from-outcome>
		    	<to-view-id>./pages/student/student-form.xhtml</to-view-id>
		    	<redirect/>
		 	</navigation-case>
		 	<!-- 
			<navigation-case>
				<from-outcome></from-outcome>
		    	<to-view-id>/page.xhtml</to-view-id>
		    	<redirect/>
			</navigation-case>
			-->
		</navigation-rule>
</faces-config>